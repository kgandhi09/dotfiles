{

    "languageserver": {
        "dart": {
            "command": "dart",
            "args": [
            "/usr/local/flutter/bin/cache/dart-sdk/bin/snapshots/analysis_server.dart.snapshot",
            "--lsp"
            ],
            "filetypes": ["dart"],
            // "disableDynamicRegister": true,
            "trace.server": "verbose"
        }
  },

    "hover.target": "float",
    "hover.floatConfig": {
        "border": true,
        "highlight": "CocFloating",
    },

    "colors.enable": true,

    //c-cpp configuration
    "clangd.enabled": true,
    "clangd.path": "/bin/clangd",
    "clangd.arguments": [],
    "clangd.checkUpdates": false,
    "clangd.disableCompletion": false,
    "clangd.disableDiagnostics": false,
    "clangd.disableFileStatus": false,
    "clangd.disableSnippetCompletion": false,
    
    //flutter configuration
    "flutter.enabled": true,
    "flutter.trace.server":"verbose",
    "flutter.lsp.debug": true,
    "flutter.lsp.initialization.onlyAnalyzeProjectsWithOpenFiles": false,
    "flutter.lsp.initialization.suggestFromUnimportedLibraries": true,
    "flutter.sdk.searchPaths": [],
    "flutter.sdk.dart-lookup": "",
    "flutter.sdk.flutter-lookup": "/usr/local/flutter/",
    "flutter.provider.enableSnippet": true,
    
    //dart configuration
    "dart.analysisExcludedFolders": [],
    "dart.enableSdkFormatter": true,
    "dart.completeFunctionCalls": true,
    "dart.showTodos": true,
    
    // coc diagnostics
    "diagnostic.enable": true,
    "diagnostic.floatConfig": {
        "border": true,
        "highlight": "CocFloating",
    },
    "snippets.ultisnips.pythonPrompt": false,

    "dialog.rounded": true,


}
